global main
extern scanf
extern printf
extern exit

segment.data
annonce: db "entrez un nombre entre 1 et 10" 0ah,0 
resu: db " votre nombre est"
format: db  "%d"

segment.bss
buff: resb 1

segment .text
main:	
push annonce
call printf
add esp,4
push dword buff
push dword format
call scanf
add esp,8
mov eax,[buff]
cmp eax,5
jg greater
push 0 
call exit
greater:
push resu
call printf
add esp,4
push dword buff
push dword format
call printf
add esp,8
push 0
call exit